{
  "name": "@ctrl/deluge",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for deluge using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/deluge",
  "keywords": [
    "deluge",
    "typescript"
  ],
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "scripts": {
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:ci": "jest --runInBand --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "form-data": "2.3.3",
    "got": "9.6.0",
    "tough-cookie": "3.0.0"
  },
  "devDependencies": {
    "@types/form-data": "2.2.1",
    "@types/got": "9.4.0",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "@types/p-wait-for": "2.0.0",
    "@types/tough-cookie": "2.3.5",
    "jest": "24.0.0",
    "p-wait-for": "2.0.1",
    "prettier": "1.16.1",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "ts-jest": "23.10.5",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-xo": "0.13.0",
    "typedoc": "0.14.2",
    "typescript": "3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">= 10"
  }
}
